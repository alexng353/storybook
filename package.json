{
  "name": "@alexng353/storybook-template",
  "version": "1.0.0",
  "description": "A template for creating React components with Storybook, TailwindCSS, and Typescript",
  "packageManager": "pnpm@8.7.6",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist",
    "plugin.js",
    "plugin.d.ts"
  ],
  "scripts": {
    "build": "pnpm build:classic",
    "build:1": "pnpm clean && pnpm build:tsup",
    "build:tsup": "tsup",
    "build:classic": "rm -rf ./dist/ && pnpm build:esm && pnpm build:cjs",
    "build:cjs": "tsc -p tsconfig.lib.json",
    "build:esm": "tsc -p tsconfig.lib.json --module esnext --declaration --outDir ./dist/esm",
    "clean": "rimraf dist",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "jest",
    "lint": "eslint --fix --ext .jsx,.js,.tsx,.ts .",
    "release": "pnpm build && standard-version && git push --follow-tags origin main && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexng353/storybook.git"
  },
  "author": "EduBeyond",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexng353/storybook/issues"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "homepage": "https://github.com/alexng353/storybook#readme",
  "dependencies": {
    "@material-tailwind/react": "^2.1.4",
    "@popperjs/core": "^2.11.8",
    "classnames": "^2.3.2",
    "clsx": "^2.0.0",
    "framer-motion": "^10.16.1",
    "highlight.js": "^11.8.0",
    "react-icons": "^4.10.1",
    "react-popper": "^2.3.0",
    "react-use": "^17.4.0",
    "rimraf": "^5.0.5",
    "shade-blend-color": "^1.0.0",
    "tailwind-scrollbar-hide": "^1.1.7",
    "tailwind-variants": "^0.1.19",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@edubeyond/eslint-config-edubeyond": "latest",
    "@storybook/addon-docs": "^7.4.6",
    "@storybook/addon-essentials": "^7.4.6",
    "@storybook/addon-interactions": "^7.4.6",
    "@storybook/addon-links": "^7.4.6",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/addon-styling-webpack": "^0.0.5",
    "@storybook/blocks": "^7.4.6",
    "@storybook/react": "^7.4.6",
    "@storybook/react-vite": "^7.4.6",
    "@storybook/testing-library": "^0.2.2",
    "@tailwindcss/forms": "^0.5.6",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.7",
    "@types/shade-blend-color": "^1.0.0",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "autoprefixer": "^10.4.14",
    "babel-jest": "^29.5.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.14",
    "eslint-plugin-tailwindcss": "^3.12.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "standard-version": "^9.5.0",
    "storybook": "7.4.6",
    "tailwindcss": "^3.3.2",
    "ts-jest": "^29.1.0",
    "tsup": "^8.0.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.9"
  },
  "peerDependencies": {
    "react": "^18",
    "react-dom": "^18",
    "tailwindcss": "^3"
  }
}
